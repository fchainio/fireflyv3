{
  "name": "fireflyv3",
  "version": "3.0.0beta",
  "private": true,
  "main": "src/desktop/maindev.desktop.js",
  "scripts": {
    "start": "./node_modules/.bin/react-native start",
    "start-clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm start -- --reset-cache",
    "android": "cross-env platform=android ./node_modules/.bin/react-native run-android",
    "ios": "cross-env platform=ios ./node_modules/.bin/react-native run-ios",
    "web": "cross-env platform=web NODE_ENV=development node scripts/start.js",
    "build": "cross-env platform=web NODE_ENV=production node scripts/build.js",
    "desktop": "cross-env platform=desktop NODE_ENV=development node scripts/start_desktop.js",
    "desktop:main": "cross-env platform=desktop NODE_ENV=production node scripts/build_desktop_main.js",
    "desktop:renderer": "cross-env platform=desktop NODE_ENV=production node scripts/build_desktop_renderer.js",
    "desktop:linux": "cross-env platform=desktop  BUILD_TARGET=linux BUILD_ARCH=x64 node scripts/build_desktop.js",
    "desktop:linux32": "cross-env platform=desktop  BUILD_TARGET=linux BUILD_ARCH=ia32 node scripts/build_desktop.js",
    "desktop:mas": "cross-env platform=desktop  BUILD_TARGET=mas node scripts/build_desktop.js",
    "desktop:win32": "cross-env platform=desktop  BUILD_TARGET=win32 BUILD_ARCH=ia32 node scripts/build_desktop.js",
    "desktop:win64": "cross-env platform=desktop  BUILD_TARGET=win32 BUILD_ARCH=x64 node scripts/build_desktop.js",
    "test": "npm run test:web && npm run test:native",
    "test:web-watch": "node scripts/test.js --watch --config \"./config/web.jest.config.js\"",
    "test:web": "node scripts/test.js --config \"./config/web.jest.config.js\"",
    "test:native": "node scripts/test.js --config \"./config/native.jest.config.js\"",
    "test:native-watch": "node scripts/test.js --watch --config \"./config/native.jest.config.js\"",
    "coverage": "node scripts/test.js --coverage --config \"./config/web.jest.config.js\"",
    "coverage:native": "node scripts/test.js --config \"./config/native.jest.config.js\" --coverage"
  },
